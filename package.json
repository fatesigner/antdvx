{
  "name": "antdvx",
  "version": "0.0.0",
  "description": "基于 ant design vue 的 Vue3 组件库，包含一些通用模板和服务",
  "license": "MIT",
  "keywords": [
    "antd",
    "vue",
    "typescript",
    "admin"
  ],
  "author": {
    "name": "Jeremy Tang",
    "email": "fatesigner@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fatesigner/antdvx.git"
  },
  "bugs": {
    "url": "https://github.com/fatesigner/antdvx/issues"
  },
  "homepage": "https://github.com/fatesigner/antdvx#readme",
  "main": "./antdv.js",
  "module": "./antdv.js",
  "types": "./antdv.d.ts",
  "scripts": {
    "preview": "vite preview",
    "start": "cross-env mode=dev npm run webpack-serve",
    "start-test": "cross-env mode=test npm run webpack-serve",
    "start-prod": "cross-env mode=prod npm run webpack-serve",
    "build": "gulp build --gulpfile build/gulpfile.js",
    "build-test": "cross-env mode=test npm run webpack-build",
    "build-prod": "cross-env mode=prod npm run webpack-build",
    "build-icons": "gulp build-icons --gulpfile build/gulpfile.js",
    "api": "cross-env mode=dev node build/scripts/set-env.js gulp api --gulpfile build/gulpfile.js",
    "generate": "cross-env mode=dev node build/scripts/set-env.js node build/generate/generate.js",
    "webpack": "cross-env NODE_ENV=development node build/scripts/set-env.js vite",
    "webpack-build": "cross-env NODE_ENV=production node build/scripts/set-env.js vite build",
    "webpack-serve": "cross-env NODE_ENV=development node build/scripts/set-env.js vite",
    "browserslist": "npx browserslist",
    "update": "ncu -u --target minor",
    "prepare": "npx husky install",
    "commit": "npx git-cz",
    "lint": "lint-staged",
    "test": "jest --detectOpenHandles"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 7.0.0"
  },
